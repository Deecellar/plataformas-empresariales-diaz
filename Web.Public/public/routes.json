{
    "routes": [
        {
          "route": "/*",
          "serve": "/index.html",
          "statusCode": 200
        }
      ],
    "mimeTypes": {
	"js":	"application/x-javascript",
    "manifest": "text/cache-manifest",
	"html": "text/html",
    "png": "image/png",
	"jpg": "image/jpg",
	"svg":	"image/svg+xml",
    "css":	"text/css",
    "module": "application/x-javascript"
    }
}